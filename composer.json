{
    "name": "fredjuvaux/doctrine-orm-provider",
    "description": "Doctrine ORM provider for Silex",
    "keywords": ["silex", "doctrine", "orm", "database", "extension", "provider"],
    "license": "MIT",
    "homepage": "https://github.com/fredjuvaux/doctrine-orm-provider",
    "authors": [
        { 
            "name": "Fred Juvaux",
            "homepage": "http://fredjuvaux.fr/",
            "email": "hello@fredjuvaux.fr" 
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": "2.1.*",
        "doctrine/common": "2.1.*",
        "doctrine/dbal": "2.1.*",
        "symfony/doctrine-bridge": "~2.3",
    },
    "suggest": {
        "symfony/console": "Necessary if you would like to use the Doctrine CLI tools"
    },
    "autoload": {
        "psr-0": { 
            "SilexORM\\Provider": "lib/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
